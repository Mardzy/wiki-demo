{
  "name": "react-apollo-graphql-server",
  "version": "1.0.0",
  "description": "boilerplate for apollo graphql server",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx webpack",
    "start": "yarn build && yarn serve",
    "serve": "nodemon ./dist/index.js"
  },
  "author": "Paul Mardling",
  "license": "ISC",
  "dependencies": {
    "graphql": "^14.5.8",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/lodash": "^4.14.165",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "apollo-datasource-rest": "^0.9.5",
    "apollo-server": "^2.19.0",
    "eslint": "^7.14.0",
    "graphql-tag": "^2.11.0",
    "nodemon": "^2.0.6",
    "ntypescript": "^1.201706190042.1",
    "prettier": "^2.2.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "tslint": "^5.20.0",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "pre-push": ""
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  }
}
